    <validationRules>
        <fullName>name</fullName>
        <active>false</active>
        <errorConditionFormula>NOT( REGEX(Name,&quot;([A-Z]{2}[0-9]{2}+.*)&quot;) )</errorConditionFormula>
        <errorDisplayField>Name</errorDisplayField>
        <errorMessage>once check name and type properly</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>phone</fullName>
        <active>false</active>
        <errorConditionFormula>Phone  &lt;&gt; null &amp;&amp;  NOT( REGEX(Phone ,&quot;[+]{1}[9]{1}[1]{1}[7-9]{1}[0-9]{9}&quot;) )</errorConditionFormula>
        <errorDisplayField>Phone</errorDisplayField>
        <errorMessage>you must type proper phone no</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>phone_update</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Type ,&quot;prospect&quot;) &amp;&amp;  Phone  = &quot;&quot;</errorConditionFormula>
        <errorMessage>u must enter phone no</errorMessage>
    </validationRules>
